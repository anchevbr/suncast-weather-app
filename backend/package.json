{
  "name": "suncast-backend",
  "version": "1.0.0",
  "description": "Backend cache server for Suncast historical sunset data",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "cache:stats": "node -e \"import('./cacheManager.js').then(m => m.getCacheStats().then(console.log))\"",
    "cache:clear": "node -e \"import('./cacheManager.js').then(m => m.clearAllCache())\""
  },
  "keywords": [
    "sunset",
    "weather",
    "cache",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
